{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "crud-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/crud/CrudWebPart.js",
          "manifest": "./src/webparts/crud/CrudWebPart.manifest.json"
        }
      ]
    },
    "chart-js-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/chartJs/ChartJsWebPart.js",
          "manifest": "./src/webparts/chartJs/ChartJsWebPart.manifest.json"
        }
      ]
    },
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "CrudWebPartStrings": "lib/webparts/crud/loc/{locale}.js",
    "ChartJsWebPartStrings": "lib/webparts/chartJs/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js"
  }
}
